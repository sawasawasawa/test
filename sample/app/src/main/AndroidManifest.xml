<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.activitystarter">

    <uses-permission android:name="android.permission.SET_DEBUG_APP" />

    <application
        android:allowBackup="false"
        android:fullBackupContent="false"
        android:label="@string/app_name"
        tools:ignore="MissingApplicationIcon,UnusedAttribute">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StudentDataActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".parcelable.StudentParcelableActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".parceler.StudentParcelerActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AllTypesActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".serializable.StudentSerializableActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".fragment.TabbedFragmentActivity"
            android:label="@string/title_activity_tabbed_fragment"
            android:theme="@style/AppTheme.NoActionBar" />

        <service
            android:name=".SomeService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>